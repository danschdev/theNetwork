{% extends 'base.html.twig' %}
{% block body %}
    {% for post in posts  %}
        <div>
            <h1 class="post-data">{{ post.createdDate|date("d.m.Y H:i:s") }} <span class="post-votes">{{ post.votesString }}</span></h1>
            {{ post.content }}
        </div>
    {% endfor %}
{% endblock %}